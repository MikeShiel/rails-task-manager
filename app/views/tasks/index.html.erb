<div class="container">
<h1>Your Tasks</h1>
<ul>
<% @tasks.each do |task| %>

  <li><%= task.name %>
    <%= link_to "show", task_path(task) %>
    <%= link_to "destroy", task_path(task), method: :delete  %>
    <%= link_to "edit", edit_task_path(task)  %>
  </li>

<% end %>
</ul>
<%= link_to "new_task", new_task_path %>
</div>
